    "iis": {
      "applicationUrl": "http://localhost/HelloWorldDocker",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:47573",
      "sslPort": 44371
    }
  },
  "profiles": {
